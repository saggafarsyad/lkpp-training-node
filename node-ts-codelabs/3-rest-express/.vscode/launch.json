  "version": "0.2.0",
  "configurations": [
    {
			"name": "todo-api-v2",
			"type": "node",
			"request": "launch",
			"args": ["${workspaceRoot}/todo-api-v2/index.ts"],
			"runtimeArgs": ["-r", "ts-node/register"],
			"cwd": "${workspaceRoot}",
			"protocol": "inspector",
			"internalConsoleOptions": "openOnSessionStart",
